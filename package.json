  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.3",
    "@nuxt/test-utils": "^3.10.0",
    "@types/node": "^20.11.5",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.56.0",
    "happy-dom": "^13.2.1",
    "nuxt": "^3.9.3",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.2.1",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  },
  "resolutions": {
    "@nuxt/kit": "3.9.3",
    "@nuxt/schema": "3.9.3",
    "tailwindcss": "3.4.1",
    "vue": "3.3.13"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.3",
    "@nuxtjs/tailwindcss": "^6.11.0",
    "tailwindcss": "^3.4.1"
  }
}
